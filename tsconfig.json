{
  "compilerOptions": {
    "target": "ES2017",
    "jsx": "react",
    "module": "commonjs",
    "resolveJsonModule": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["./node_modules/@types"],
    "noUnusedLocals": false,
    "noUnusedParameters": false
  },
  "include": ["./src/**/*.*"],
  "exclude": ["./src/generated/*.*"]
}
